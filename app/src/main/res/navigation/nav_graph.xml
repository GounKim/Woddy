<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/fragmentFirst">
    <fragment
        android:id="@+id/fragmentFirst"
        android:name="com.example.woddy.FragmentHome"
        android:label="fragment_first"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_fragmentFirst_to_fragmentSecond"
            app:destination="@id/fragmentSecond" />
    </fragment>
    <fragment
        android:id="@+id/fragmentSecond"
        android:name="com.example.woddy.FragmentPost"
        android:label="fragment_second"
        tools:layout="@layout/fragment_post" >
        <action
            android:id="@+id/action_fragmentSecond_to_fragmentThird"
            app:destination="@id/fragmentThird" />
    </fragment>
    <fragment
        android:id="@+id/fragmentThird"
        android:name="com.example.woddy.FragmentChatting"
        android:label="fragment_third"
        tools:layout="@layout/fragment_chatting" >
        <action
            android:id="@+id/action_fragmentThird_to_fragmentFourth"
            app:destination="@id/fragmentFourth" />
    </fragment>
    <fragment
        android:id="@+id/fragmentFourth"
        android:name="com.example.woddy.FragmentFourth"
        android:label="fragment_fourth"
        tools:layout="@layout/fragment_my" />
